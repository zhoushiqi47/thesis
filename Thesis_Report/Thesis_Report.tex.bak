\subsection{算法分析与思考}
\begin{frame}
\frametitle{算法分析与思考}
\begin{block}{有限孔穴点扩散函数}
\begin{equation*}
\textcolor{red}{
  J_d(z,y):=\int_{\Gamma_0^d}\frac{\partial G(x_r,z)}{\partial x_2(x_r)}\overline{N(x_r,y)}ds(x_r)
  }
\end{equation*}
其中函数$G(x,z),N(x,y)$分别为Pekeris开波导Dirichlet和Neumann格林函数。
\end{block}
\begin{small}
\begin{figure}[htbp]
  \centering
  \includegraphics[width=5cm,height=2.5cm]{./waveguide1/psf_waveguide/in_mesh.eps}
  \includegraphics[width=5cm,height=2.5cm]{./waveguide1/psf_waveguide/out_mesh.eps}
  %\includegraphics[width=12cm,height=6cm]{./waveguide1/psf_half/out_mesh.eps}
  \caption{\tiny{$-\Im J_d(z,y)$，其中$\textcolor{red}{G_{bp}(x,z)=G(x,z)}$，左边为源点$y_1\in L_1$，右边为源点$y_2\in L_2$。 参数：$k_1=4\pi,k_2=2\pi,h=10,d=50,N_r=256,y_1=(0,8),y_2=(0,12)$.}}
\end{figure}
\end{small}
\end{frame}

\begin{frame}
\frametitle{点扩散函数讨论}
\begin{small}
函数$G(x,z)$和$N(x,y)$都会产生波导模式：
\begin{eqnarray*}
  G(x,z)=G_g(x,z)+G_{rad}(x,z),\ \
  N(x,y)=N_g(x,y)+N_{rad}(x,y)
\end{eqnarray*}
其中$G_g(x,z)=\sum\limits_{p=1}^MG_p(x,z)$ 和$N_g(x,y)=\sum\limits_{q=1}^{\hat M}N_q(x,y)$，分别为$G(x,z)$和$N(x,y)$的波导项，$G_{rad}(x,z)$和$N_{rad}(x,y)$分别为相应衰减项。则
\begin{equation*}
  J_d(z,y)=\textcolor{red}{J_d^1(z,y)}+J_d^2(z,y)+J_d^3(z,y)+J_d^4(z,y)
\end{equation*}
其中$J_d^i(z,y),i=1,2,3,4$为
\begin{eqnarray*}\label{psf_wg_4p}
 \left\{
 \begin{array}{lll}
  \textcolor{red}{J_d^1(z,y)}&\textcolor{red}{=}&\textcolor{red}{\int_{\Gamma_0^d}\frac{\partial G_g(x_r,z)}{\partial x_2(x_r)}\overline{N_g(x_r,y)}ds(x_r)}\\
  & &\\
  J_d^2(z,y)&=&\int_{\Gamma_0^d}\frac{\partial G_{rad}(x_r,z)}{\partial x_2(x_r)}\overline{N_{rad}(x_r,y)}ds(x_r)\\
  & &\\
  J_d^3(z,y)&=&\int_{\Gamma_0^d}\frac{\partial G_g(x_r,z)}{\partial x_2(x_r)}\overline{N_{rad}(x_r,y)}ds(x_r)\\
  & &\\
  J_d^4(z,y)&=&\int_{\Gamma_0^d}\frac{\partial G_{rad}(x_r,z)}{\partial x_2(x_r)}\overline{N_g(x_r,y)}ds(x_r)
 \end{array}
 \right.
\end{eqnarray*}
\end{small}
\end{frame}

