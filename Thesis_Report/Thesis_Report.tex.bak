\begin{frame}
\frametitle{分辨率分析证明思路}
\begin{small}
\begin{block}{目标：建立成像函数$I^{\rm Phaseless}_{\rm RTM}(z)$的分辨率分析}
\begin{eqnarray*}
 I^{\rm Phaseless}_{\rm RTM}(z)&=& \textcolor{red}{I_{\rm RTM}(z)}+\textcolor{blue}{{\rm I}_2(z)}
  +\textcolor{blue}{{\rm I}_3(z)}
\end{eqnarray*}
\end{block}

\begin{remark}[半空间RTM成像函数$I_{\rm RTM}$的分辨率分析]
对任意的 $z\in\Omega$， 令$\psi(y,z)$为如下问题的散射解 $$
\Delta_y\psi(y,z)+k^2n(y)\psi(y,z)=-k^2(n(y)-1)\overline{F(z,y)}\ \ in\ \ \R^2.$$
则我们有， 对任意的 $z\in\Omega$,$$
\textcolor{red}{I_{\rm RTM}(z)}=-\frac{1}{4}\Im\int_Dk^2(1-n(y))(\psi(y,z)+\overline{F(z,y)})\overline{F(z,y)}dy
+W_{\hat{I}}(z),$$
其中 $\left|W_{\hat{I}}(z)\right|\leq C(1+kd_D)^4((kh)^{-1/2}+h/d)$，关于$z\in\Omega$一致成立。

\end{remark}
\begin{block}{函数$F(z,y):=-\frac{\ii}{2\pi}\int_0^{\pi}e^{\i k(z_1-y_1)\cos\theta+\i k(z_2-y_2)\sin\theta}d\theta$具有如下性质：}
存在某个与$k$无关的常数$C$使得，
\begin{equation*}
|F(z,y)|\leq C[(k|z-y|)^{-1/2}+(k|z-y|)^{-1}],\ \ \forall z,y\in\Omega.
\end{equation*}
\end{block}
\end{small}
\end{frame}
\begin{frame}
\frametitle{算法分辨率分析}
\begin{small}
\begin{them}[无相位逆时偏移算法的分辨率分析]
 设$\Omega$\footnote[1]{关于采样区域$\Omega$的假设：$|y_1-z_1|\leq Ch,\ z_2\leq Ch,\ \ \forall y,z\in\Omega,\ \ \mbox{其中 }h=dist(\Omega,\Gamma_0)$
}为采样区域，若$|u(x_r,x_s)|=|u^s(x_r,x_s)+u^i(x_r,x_s))|$为所测量的散射总场的无相位数据，其中 $u^s(x,x_s)$ 是以$u^i(x,x_s)=N_k(x,x_s)$ 为入射场的散射解，则我们有
\begin{equation*}
\textcolor{red}{
  I^{\rm phaseless}_{\rm RTM}(z)=I_{\rm RTM}(z)+ R^{\rm phaseless}_{\rm RTM}(z),\ \ \forall z\in\Omega,
  }
\end{equation*}
其中
\begin{equation*}
\textcolor{blue}{
  \left| R^{\rm phaseless}_{\rm RTM}(z)\right|\leq C(1 + \|\mathbb{S}\|)^2(kh)^{-1/2},\ \ \forall z\in\Omega.
  }
\end{equation*}
其中常数$C>0$ 可能依赖于 $kd_D$ 和 $\|n(\cdot)\|_{L^{\infty}(D)}$，但是与 $k,h,d_D$无关。
\end{them}

\begin{remark}[$\mathbb{S}$为正问题的解算子]
$\mathbb{S}:L^2(D)\rightarrow H^1(D)$,
\begin{equation*}
  w=\mathbb{S} g
\end{equation*}
其中$w\in H^1(D)$ 为对应于入射场$g\in L^2(D)$的散射场，其满足如下方程：
\begin{equation*}\label{forward}
  \Delta w+k^2n(x)w=k^2(1-n(x))g\  \ in\  \ \R^2_+, \  \ \frac{\partial w}{\partial x_2}=0\  \  on\  \  \Gamma_0,
\end{equation*}
\end{remark}
\end{small}
\end{frame}
\section{Pekeris开波导障碍物成像问题}
\subsection{Pekeris模型和问题介绍}
\begin{frame}
\frametitle{Pekeris开波导模型}
\begin{columns}
\begin{column}{.5\textwidth}

\begin{small}
\begin{eqnarray*}
\left\{
\begin{array}{lll}
    \Delta u+\textcolor{red}{k(x)}^2n(x)u=-\delta_{x_s}(x),&in&\R^2_+\\
    & &\\
    \left[u(\cdot,x_s)\right]_{\Gamma_h}=\left[\frac{\partial u(\cdot,x_s)}{\partial\nu}\right]_{\Gamma_h}=0\\
    & &\\
    \frac{\partial u}{\partial x_2}=0,&on&\Gamma_0\\
\end{array}
\right.
\end{eqnarray*}
\end{small}
\small{其中$n(x)-1$为支集在障碍物$D$上的有界函数。}

\begin{figure}
  \centering
  \includegraphics[width=6cm,height=3cm]{./figure/wg_forward}
\end{figure}
\end{column}

\begin{column}{.5\textwidth}
\begin{block}{符号说明}
\begin{small}
背景介质波数$k(x)$ 为
\begin{eqnarray*}
k(x)=\left\{
\begin{array}{lll}
  k_1&,&x_2\in(0,h)\\
  & &\\
  k_2&,&x_2\in(h,+\infty)
\end{array}
\right.
\end{eqnarray*}
其中$\textcolor{red}{k_1>k_2}$，并记$\Gamma_0,\Gamma_h,L_1,L_2$ 分别为
\begin{itemize}
  \item $ \Gamma_0=\{(x_1,x_2);x_1\in \R, x_2=0\}$;
  \item $ \Gamma_h=\{(x_1,x_2);x_1\in \R, x_2=h\}$;
  \item $ L_1=\{(x_1,x_2);x_1\in \R, 0<x_2<h\}$;
  \item $ L_2=\{(x_1,x_2);x_1\in \R, x_2>h\}$.
\end{itemize}
\end{small}
\end{block}
\begin{block}{开波导逆散射问题}
\begin{small}
在$\Gamma_0$上接受散射数据，然后构建合适的数值重构算法，来确定障碍物$D$位置、大小和形状。
\end{small}
\end{block}
\end{column}
\end{columns}
\end{frame}

\begin{frame}
\frametitle{散射问题}
\begin{columns}
\begin{column}{.8\textwidth}
\begin{block}{入射场}
\begin{small}
令$u^i(x,x_s)=N(x,x_s)$为入射场，其中$N(x,y)$为Pekeris开波导Neumann格林函数，其满足如下方程：
\begin{eqnarray*}\label{G_Neumann}
\left\{
\begin{array}{lll}
  \Delta_xN(x,y)+k^2(x)N(x,y)=-\delta_y(x),&in&\R^2_+\\
  & &\\
  \left[N(\cdot,y)\right]_{\Gamma_h}=0,\ \ \left[\frac{\partial N(\cdot,y)}{\partial\nu}\right]_{\Gamma_h}=0 & &\\
  & &\\
  \frac{\partial N(x,y)}{\partial x_2}=0, &on&\Gamma_0
\end{array}
\right.
\end{eqnarray*}
\end{small}
\end{block}
\begin{block}{散射场}
\begin{small}
散射场$u^s(x,x_s)=u(x,x_s)-u^i(x,x_s)$满足如下方程：
\begin{eqnarray*}\label{wg_scatterfield}
\left\{
\begin{array}{lll}
 \Delta u^s(x,x_s)+k^2(x)u^s(x,x_s)=k^2(x)[1-n(x)]u(x,x_s),&in&\R^2_+ \\
  & &\\
  \left[u^s(\cdot,x_s)\right]_{\Gamma_h}=0,\ \ \left[\frac{\partial u^s(\cdot,x_s)}{\partial\nu}\right]_{\Gamma_h}=0.\\
 & &\\
 \frac{\partial u^s(x,x_s)}{\partial x_2}=0,&on&\Gamma_0
\end{array}
\right.
\end{eqnarray*}
\end{small}
\end{block}
\end{column}
\begin{column}{.2\textwidth}
\begin{small}
\begin{block}{开波导散射问题注意事项}
\begin{enumerate}
  \item 开波导格林函数$N(x,y)$积分表达式和波导模式展开的推导及数值计算
  \item 辐射条件的提出以及正问题的适定性
  \item 正散射问题的数值计算方法
\end{enumerate}
\end{block}
\end{small}
\end{column}
\end{columns}
\end{frame}

\begin{frame}
\frametitle{格林函数$N(x,y)$积分表达式}
\begin{small}
令$\xi=\xi_1+\i\xi_2,\xi_1,\xi_2\in\R$，以及$\mu_j=\sqrt{k_j^2-\xi^2},j=1,2$，并记关于$\xi$的函数$\hat N_h(\xi)$ 和$\hat M_h(\xi)$ 如下所示：
\begin{eqnarray*}\label{NhMh_N}
 \textcolor{red}{\hat N_h(\xi)=\frac{\mu_1+\mu_2}{\mu_1-\mu_2}-e^{2\i\mu_1h}},\ \
 \hat M_h(\xi)=\frac{\mu_1+\mu_2}{\mu_1-\mu_2}e^{2\i\mu_1h}-1
\end{eqnarray*}
则格林函数$N(x,y)$有如下表达式
\begin{eqnarray*}
 N(x,y)=\left\{
 \begin{array}{lll}
   \Phi_{k_1}(x,y)+\Phi_{k_1}(x,y')+\hat S_1(x,y)&,&x\in L_1,y\in L_1\\
   \hat S_2(x,y)&,&x\in L_2,y\in L_1\\
   \hat K_1(x,y)&,&x\in L_1,y\in L_2\\
   \Phi_{k_2}(x,y)+\hat K_2(x,y)&,&x\in L_2,y\in L_2
 \end{array}
 \right.
\end{eqnarray*}
其中$\hat S_j(x,y),\hat K_j(x,y)$的表达式为
\begin{eqnarray*}
\left\{
\begin{array}{lll}
 \hat{S}_1(x,y)&=&\frac{1}{2\pi}\int_{SIP}\frac{\i}{2\mu_1}\frac{e^{2\i\mu_1h}
 \left[4\cos{(\mu_1x_2)\cos{(\mu_1y_2)}}\right]}{\textcolor{red}{\hat N_h(\xi)}}
  e^{\i(x_1-y_1)\xi}d\xi\\
  & &\\
 \hat{S}_2(x,y)&=&\frac{1}{2\pi}\int_{SIP}\frac{2\i e^{\i\mu_1h}\cos{(\mu_1y_2)}}{(\mu_1-\mu_2)\textcolor{red}{\hat N_h(\xi)}}e^{\i\mu_2(x_2-h)+\i(x_1-y_1)\xi}d\xi\\
 & &\\
 \hat{K}_1(x,y)&=&\frac{1}{2\pi}\int_{SIP}\frac{2\i e^{\i\mu_1h}\cos(\mu_1x_2)}{\textcolor{red}{\hat N_h(\xi)}}e^{\i\mu_2(y_2-h)+\i(x_1-y_1)\xi}d\xi\\
 & &\\
 \hat{K}_2(x,y)&=&\frac{1}{2\pi}\int_{SIP}\frac{\i}{2\mu_2}\frac{\hat M_h(\xi)}{\textcolor{red}{\hat N_h(\xi)}}e^{\i\mu_2(x_2+y_2-2h)+\i(x_1-y_1)\xi}d\xi
\end{array}
\right.
\end{eqnarray*}
\end{small}
\end{frame}


\begin{frame}
\frametitle{提取波导模式}
\begin{columns}

\begin{column}{0.5\textwidth}
\begin{figure}[htbp]
  \centering
  \includegraphics[width=6cm,height=3cm]{./SIP/SIP_IFT.jpg}
  \caption{Sommerfeld积分路径(SIP)}\label{SIP_IFT}
\end{figure}
\end{column}

\begin{column}{0.5\textwidth}
\begin{tiny}
\begin{block}{解析分支}
对于任意地$z\in \mathbb{C}\backslash\{0\}$， $z^{1/2}$是$\sqrt{z}$ 的如下解析分支：$\Im(z^{1/2})\geq 0$，
则对于$z=z_1+\mathbf{i}z_2$, $z_1,z_2\in\R$,
\begin{equation*}\label{branch}
  z^{1/2}=sgn(z_2)\sqrt{\frac{|z|+z_1}{2}}+\mathbf{i}\sqrt{\frac{|z|-z_1}{2}}
\end{equation*}
当$z$位于右半实轴时，取$z^{1/2}$为$\epsilon\rightarrow0^+$ 时$(z+\i\epsilon)^{1/2}$ 的极限即可。
\end{block}
\end{tiny}
\end{column}
\end{columns}
\begin{tiny}
\begin{block}{函数$\hat N_h(\xi)$在实轴上的零点}

\begin{enumerate}
  \item 当$\xi\in\{\xi\in\R;|\xi|\geq k_1,|\xi|<k_2\}$时，$\hat N_h(\xi)\neq0$。
  \item 若$\sin\left(\sqrt{k_1^2-k_2^2}h\right)\neq0$，则当$\xi=\pm k_2$时，$\hat N_h(\xi)\neq0$。
  \item 当$\xi\in(k_2,k_1)$时，$\hat N_h(\xi)=0$等价于：$
\textcolor{red}{
  \tan{\left(\sqrt{k_1^2-\xi^2}h\right)}=\frac{\sqrt{\xi^2-k_2^2}}{\sqrt{k_1^2-\xi^2}}
  }
$。记函数$N_h(\xi)$在区间$(k_2,k_1)$的零点为：$\hat\xi_m,m=1,2\ldots,\hat M$，则$\hat M\leq\left[\sqrt{k_1^2-k_2^2}h/\pi\right]+1$。
\end{enumerate}
\end{block}
\end{tiny}
\end{frame}


\begin{frame}
\frametitle{格林函数$N(x,y)$的波导模式展开}
\begin{small}
设$\hat\xi_m$, $m=1,\ldots,\hat M$是$\hat N_h(\xi)$在区间$(k_2,k_1)$上的$\hat M$ 个实根，并记$\hat\mu_{jm}=\mu_j(\hat\xi_m), j=1,2$，则$N(x,y)$ 的波导模式展开为
\begin{equation*}
  \textcolor{red}{N(x,y)=N_g(x,y)+N_{rad}(x,y)}
\end{equation*}
其中$N_g(x,y)=\textcolor{red}{\sum\limits_{m=1}^{\hat M} N_m(x,y)}$，且
\begin{eqnarray*}
\left\{
\begin{array}{lll}
  N_m(x,y)&=&\frac{\hat\mu_{2m}}{\hat\xi_m(1-\i\hat\mu_{2m}h)}\hat g(x_2,\hat\xi_m)\hat g(y_2,\hat\xi_m)\textcolor{red}{e^{\i|x_1-y_1|\hat\xi_m}}\\
  & &\\
 N_{rad}(x,y)&=&\frac{\i}{\pi}\int_{\i\infty}^{k_2}\frac{\mu_2 \hat g(x_2,\xi)\hat g(y_2,\xi)}{(k_1^2-k_2^2)\sin^2(\mu_1h)+\mu_2^2}e^{\ii|x_1-y_1|\xi}d\xi
\end{array}
\right.
\end{eqnarray*}
其中$N_m(x,y)$中对应于$\hat\xi_m\in(k_2,k_1)$ 的函数$\hat g(x_2,\hat\xi_m)$表达式如下
\begin{eqnarray*}
\hat g(x_2,\hat\xi_m):=\left\{
 \begin{array}{lll}
   \cos(\hat\mu_{1m}x_2),& &x_2\in(0,h)\\
   & &\\
   \cos(\hat\mu_{1m}h)\textcolor{red}{e^{\i\hat\mu_{2m}(x_2-h)}},& &x_2\in(h,+\infty)
 \end{array}
 \right.
\end{eqnarray*}
此外，$N_{rad}(x,y)$中有向区间$[\i\infty,k_2]:=\{\xi;\xi\in[0,k_2],\ \ \mbox{或}\xi=\i\eta$,$\eta\in(0,+\infty)\}$，以及对应于$\xi\in[\i\infty,k_2]$的
函数$\hat g(x_2,\xi)$如下所示
 \begin{eqnarray*}
\hat g(x_2,\xi):=\left\{
 \begin{array}{lll}
   \cos(\mu_1x_2),& &x_2\in(0,h)\\
   & &\\
   \cos(\mu_1h)\cos[\mu_2(x_2-h)]-\frac{\mu_1}{\mu_2}\sin(\mu_1h)\sin[\mu_2(x_2-h)],& &x_2\in(h,+\infty)
 \end{array}
 \right.
 \end{eqnarray*}
\end{small}
\end{frame}

