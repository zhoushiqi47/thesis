  \includegraphics[width=5cm,height=2.5cm]{./waveguide1/psf_waveguide/out_mesh.eps}
  %\includegraphics[width=12cm,height=6cm]{./waveguide1/psf_half/out_mesh.eps}
  \caption{\tiny{$-\Im J_d(z,y)$，其中$\textcolor{red}{G_{bp}(x,z)=G(x,z)}$，左边为源点$y_1\in L_1$，右边为源点$y_2\in L_2$。 参数：$k_1=4\pi,k_2=2\pi,h=10,d=50,N_r=256,y_1=(0,8),y_2=(0,12)$.}}
\end{figure}
\end{small}
\end{frame}
\begin{frame}
\frametitle{开波导逆时偏移算法}
\begin{small}
\begin{algorithm}\label{alg_wg}
设$\Omega$为采样区域，令$u^s(x_r,x_s)$ 为在接收点$x_r$收到的由源点$x_s$ 所激发的散射数据，其中$x_r,x_s\in\Gamma_0^d;r=1,\ldots,N_r;s=1,\ldots,N_s$.\\
$1^\circ$ 反传播： 对$s=1,\ldots,N_s$，计算反传播场
\begin{equation*}
  v_b(z,x_s)=\frac{|\Gamma_0^d|}{N_r}\sum\limits_{r=1}^{N_r}\frac{\partial \textcolor{red}{G(x_r,z)}}{\partial x_2(x_r)}\overline{u^s(x_r,x_s)},\  \  \forall z\in\Omega.
\end{equation*}
若令$N_r\rightarrow0$，则上式可看做如下积分的数值近似，
\begin{equation*}
  \hat v_b(z,x_s)=\int_{\Gamma_0^d}\frac{\partial \textcolor{red}{G(x_r,z)}}{\partial x_2(x_r)}\overline{u^s(x_r,x_s)}ds(x_r)
\end{equation*}
$2^\circ$ 互相关： 对$z\in\Omega$，计算成像函数
\begin{equation*}
  I_d(z)=\Im\left\{\frac{|\Gamma_0^d|}{N_s}\sum\limits_{s=1}^{N_s}\frac{\partial \textcolor{red}{G(x_s,z)}}{\partial x_2(x_s)}v_b(z,x_s)
  \right\}.
\end{equation*}
将$v_b(z,x_s)$的表达式代入$I_d(z)$，可得
\begin{equation*}\label{Id_wg}
  I_d(z)=\Im\left\{\frac{|\Gamma_0^d|}{N_s}\frac{|\Gamma_0^d|}{N_r}\sum\limits_{s=1}^{N_s}\sum\limits_{r=1}^{N_r}\frac{\partial \textcolor{red}{G(x_s,z)}}{\partial x_2(x_s)}\frac{\partial \textcolor{red}{G(x_r,z)}}{\partial x_2(x_r)}\overline{u^s(x_r,x_s)}
  \right\}.
\end{equation*}
\end{algorithm}
\end{small}
\end{frame}

