\frametitle{点源问题及点扩散函数}

\begin{small}
\begin{block}{点源问题}
  设源点$y\in\R^2_+$，然后令$x_r,r=1,2,\ldots,N_r$ 为均匀分布在$\Gamma_0^d$ 上的$N_r$ 个接收点，如何通过在$\Gamma_0^d$ 上接收到的
  由点$y$激发而在点$x_r$ 处接收到的数据：
  $$\{N(x_r,y);r=1,2,\ldots,N_r\},$$
  来确定源点$y$的具体位置？
\end{block}
\end{small}

\begin{figure}
  \centering
  \includegraphics[width=8cm,height=4cm]{./point/point_waveguide}
\end{figure}


\end{frame}

\begin{frame}
\frametitle{有限孔穴点扩散函数}
\begin{small}
\begin{block}{离散的有限孔穴点扩散函数}
参考声波半空间逆时偏移算法，上述问题可以转化为寻求一个合适的反传播函数$G_{bp}(x,z)$使得：
\begin{equation*}
\hat J_d(z,y):=\frac{|\Gamma_0^d|}{N_r}\sum\limits_{r=1}^{N_r}\frac{\partial G_{bp}(x_r,z)}{\partial x_2(x_r)}\overline{N(x_r,y)},\ \ x_2(x_r)=0
\end{equation*}
拥有如下性质：
\begin{itemize}
  \item 当$z=y$ 时，$-\Im\hat J_d(z,y)$ 取得峰值；当$z$远离$y$ 时，$-\Im\hat J_d(z,y)$ 逐渐衰减。
\end{itemize}
\end{block}
\end{small}

\begin{small}
\begin{block}{}
当接收点的个数$N_r\rightarrow\infty$ 时，$\hat J_d(z,y)$将趋于函数$J_d(z,y)$:
\begin{equation*}
\textcolor{red}{
  J_d(z,y):=\int_{\Gamma_0^d}\frac{\partial G_{bp}(x_r,z)}{\partial x_2(x_r)}\overline{N(x_r,y)}ds(x_r)
  }
\end{equation*}
\end{block}
\end{small}

\end{frame}
\begin{frame}
\frametitle{反传播函数选取}
\begin{small}
\begin{block}{(i) 直接推广：取$G_{bp}(x,y)=G_{k_1}(x,y)$，其中函数$G_{k_1}(x,y)$满足}
\begin{eqnarray*}
\Delta_xG_{k_1}(x,y)+k_1^2G_{k_1}(x,y)=-\delta_y(x),\ \ in\ \ \R^2_+;\ \
G_{k_1}(x,y)=0,\ \ on\ \ \Gamma_0
\end{eqnarray*}
易知：$G_{k_1}(x,y)=\Phi_{k_1}(x,y)-\Phi_{k_1}(x,y')$，其中$\Phi_{k_1}(x,y)=\frac{\i}{4}H_0^{(1)}(k_1|x-y|)$.
\end{block}
\end{small}

\begin{small}
\begin{figure}[htbp]
  \centering
  \includegraphics[width=5cm,height=2.5cm]{./waveguide1/psf_half/in_mesh.eps}
  \includegraphics[width=5cm,height=2.5cm]{./waveguide1/psf_half/out_mesh.eps}
  %\includegraphics[width=12cm,height=6cm]{./waveguide1/psf_half/out_mesh.eps}
  \caption{\tiny{$-\Im J_d(z,y)$，其中$\textcolor{red}{G_{bp}(x,z)=G_{k_1}(x,z)}$，左边为源点$y_1\in L_1$，右边为源点$y_2\in L_2$。参数：$k_1=4\pi,k_2=2\pi,h=10,d=50,N_r=256,y_1=(0,8),y_2=(0,12)$.}}
\end{figure}
\end{small}
\end{frame}

\begin{frame}
\frametitle{另一种选取方法}
\begin{small}
\begin{block}{(ii) 取$G_{bp}(x,y)=G(x,y)$，其中函数$G(x,y)$满足}
\begin{eqnarray*}\label{G_Dirichlet}
\left\{
\begin{array}{lll}
  \Delta_xG(x,y)+k^2(x)G(x,y)=-\delta_z(y),&in&\R^2_+\\
  & &\\
  \left[G(\cdot,y)\right]_{\Gamma_h}=0,\ \ \left[\frac{\partial G(\cdot,y)}{\partial\nu}\right]_{\Gamma_h}=0 & &\\
  & &\\
  G(x,y)=0, &on&\Gamma_0
\end{array}
\right.
\end{eqnarray*}
其中背景介质波数$k(x)$ 为
\begin{eqnarray*}
k(x)=\left\{
\begin{array}{lll}
  k_1&,&x_2\in(0,h)\\
  & &\\
  k_2&,&x_2\in(h,+\infty)
\end{array}
\right.
\end{eqnarray*}
函数$G(x,y)$被称为Pekeris开波导Dirichlet格林函数。
\end{block}
\end{small}
\end{frame}

\begin{frame}
\frametitle{格林函数$G(x,y)$的积分表达式}
\begin{small}
令$\xi=\xi_1+\i\xi_2,\xi_1,\xi_2\in\R$，以及$\mu_j=\sqrt{k_j^2-\xi^2},j=1,2$，并记关于$\xi$的函数$N_h(\xi)$和$M_h(\xi)$如下所示
\begin{eqnarray*}\label{NhMh}
 \textcolor{red}{N_h(\xi)=\frac{\mu_1+\mu_2}{\mu_1-\mu_2}+e^{2\i\mu_1h}},\ \
 M_h(\xi)=1+\frac{\mu_1+\mu_2}{\mu_1-\mu_2}e^{2\i\mu_1h},
\end{eqnarray*}
则格林函数$G(x,y)$有如下表达式
\begin{eqnarray*}
G(x,y)=\left\{
\begin{array}{lll}
  \Phi_{k_1}(x,y)-\Phi_{k_1}(x,y')+S_1(x,y)&, &x\in L_1,y\in L_1\\
 S_2(x,y)&, &x\in L_2,y\in L_1\\
 K_1(x,y)&, &x\in L_1,y\in L_2\\
 \Phi_{k_2}(x,y)+K_2(x,y)&,&x\in L_2,y\in L_2
\end{array}
\right.
\end{eqnarray*}
其中$S_j(x,y),K_j(x,y),j=1,2$ 的表达式为
\begin{eqnarray*}
\left\{
\begin{array}{lll}
  S_1(x,y)&=&\frac{1}{2\pi}\int_{SIP}\frac{-\i}{2\mu_1}\frac{e^{2\i\mu_1h}\left[4\sin{(\mu_1x_2)\sin{(\mu_1y_2)}}\right]}{\textcolor{red}{N_h(\xi)}}
  e^{\i(x_1-y_1)\xi}d\xi\\
  & &\\
  S_2(x,y)&=&\frac{1}{2\pi}\int_{SIP}\frac{2e^{\i\mu_1h}\sin{(\mu_1y_2)}}{(\mu_1-\mu_2)\textcolor{red}{N_h(\xi)}}e^{\i\mu_2(x_2-h)+\i(x_1-y_1)\xi}d\xi\\
  & &\\
  K_1(x,y)&=&\frac{1}{2\pi}\int_{SIP}\frac{2e^{\i\mu_1h}\sin(\mu_1x_2)}{(\mu_1-\mu_2)\textcolor{red}{N_h(\xi)}}e^{\i\mu_2(y_2-h)+\i(x_1-y_1)\xi}d\xi\\
  & &\\
  K_2(x,y)&=&\frac{1}{2\pi}\int_{SIP}\frac{-\i}{2\mu_2}\frac{M_h(\xi)}{\textcolor{red}{N_h(\xi)}}e^{\i\mu_2(x_2+y_2-2h)+\i(x_1-y_1)\xi}d\xi
\end{array}
\right.
\end{eqnarray*}
\end{small}

\end{frame}

\begin{frame}
\frametitle{格林函数$G(x,y)$的波导模式展开}
\begin{small}
设$\xi_m$, $m=1,\ldots,M$是$\textcolor{blue}{N_h(\xi)}$在区间$(k_2,k_1)$ 上的$M$ 个实根，并记$\mu_{jm}=\mu_j(\xi_m), j=1,2$，则$G(x,y)$ 的波导模式展开为
\begin{equation*}
\textcolor{red}{
  G(x,y)=G_g(x,y)+G_{rad}(x,y)
  }
\end{equation*}
其中$G_g(x,y)=\textcolor{red}{\sum\limits_{m=1}^M G_m(x,y)}$，且
\begin{eqnarray*}
\left\{
\begin{array}{lll}
  G_m(x,y)&=&\frac{\mu_{2m}}{\xi_m(1-\i\mu_{2m}h)}g(x_2,\xi_m)g(y_2,\xi_m)\textcolor{red}{e^{\i|x_1-y_1|\xi_m}}\\
& &\\
 G_{rad}(x,y)&=&\frac{\ii}{\pi}\int_{\ii\infty}^{k_2}\frac{\mu_2 g(x_2,\xi)g(y_2,\xi)}{(k_1^2-k_2^2)\cos^2(\mu_1h)+\mu_2^2}e^{\ii|x_1-y_1|\xi}d\xi
\end{array}
\right.
\end{eqnarray*}
其中$G_m(x,y)$中对应于$\xi_m\in(k_2,k_1)$的$g(x_2,\xi_m)$ 表达式如下
 \begin{eqnarray*}
 g(x_2,\xi_m):=\left\{
 \begin{array}{lll}
   \sin(\mu_{1m}x_2),& &x_2\in(0,h)\\
   & &\\
   \sin(\mu_{1m}h)\textcolor{red}{e^{\i\mu_{2m}(x_2-h)}},& &x_2\in(h,+\infty)
 \end{array}
 \right.
 \end{eqnarray*}
此外$G_{rad}(x,y)$中有向积分区间$[\i\infty,k_2]:=\{\xi;\xi\in[0,k_2],\ \ \mbox{或}\xi=\i\eta$,$\eta\in(0,+\infty)\}$，以及对应于$\xi\in[\i\infty,k_2]$的函数$g(x_2,\xi)$ 如下
 \begin{eqnarray*}
 g(x_2,\xi):=\left\{
 \begin{array}{lll}
   \sin(\mu_1x_2),& &x_2\in(0,h)\\
   & &\\
   \sin(\mu_1h)\cos[\mu_2(x_2-h)]+\frac{\mu_1}{\mu_2}\cos(\mu_1h)\sin[\mu_2(x_2-h)],& &x_2\in(h,+\infty)
 \end{array}
 \right.
 \end{eqnarray*}
\end{small}

\end{frame}


\begin{frame}
\frametitle{回顾：格林函数$N(x,y)$的波导模式展开}
\begin{small}
设$\hat\xi_m$, $m=1,\ldots,\hat M$是$\textcolor{blue}{\hat N_h(\xi)}$在区间$(k_2,k_1)$上的$\hat M$ 个实根，并记$\hat\mu_{jm}=\mu_j(\hat\xi_m), j=1,2$，则$N(x,y)$ 的波导模式展开为
\begin{equation*}
  \textcolor{red}{N(x,y)=N_g(x,y)+N_{rad}(x,y)}
\end{equation*}
其中$N_g(x,y)=\textcolor{red}{\sum\limits_{m=1}^{\hat M} N_m(x,y)}$，且
\begin{eqnarray*}
\left\{
\begin{array}{lll}
  N_m(x,y)&=&\frac{\hat\mu_{2m}}{\hat\xi_m(1-\i\hat\mu_{2m}h)}\hat g(x_2,\hat\xi_m)\hat g(y_2,\hat\xi_m)\textcolor{red}{e^{\i|x_1-y_1|\hat\xi_m}}\\
  & &\\
 N_{rad}(x,y)&=&\frac{\i}{\pi}\int_{\i\infty}^{k_2}\frac{\mu_2 \hat g(x_2,\xi)\hat g(y_2,\xi)}{(k_1^2-k_2^2)\sin^2(\mu_1h)+\mu_2^2}e^{\ii|x_1-y_1|\xi}d\xi
\end{array}
\right.
\end{eqnarray*}
其中$N_m(x,y)$中对应于$\hat\xi_m\in(k_2,k_1)$ 的函数$\hat g(x_2,\hat\xi_m)$表达式如下
\begin{eqnarray*}
\hat g(x_2,\hat\xi_m):=\left\{
 \begin{array}{lll}
   \cos(\hat\mu_{1m}x_2),& &x_2\in(0,h)\\
   & &\\
   \cos(\hat\mu_{1m}h)\textcolor{red}{e^{\i\hat\mu_{2m}(x_2-h)}},& &x_2\in(h,+\infty)
 \end{array}
 \right.
\end{eqnarray*}
此外，$N_{rad}(x,y)$中有向区间$[\i\infty,k_2]:=\{\xi;\xi\in[0,k_2],\ \ \mbox{或}\xi=\i\eta$,$\eta\in(0,+\infty)\}$，以及对应于$\xi\in[\i\infty,k_2]$的
函数$\hat g(x_2,\xi)$如下所示
 \begin{eqnarray*}
\hat g(x_2,\xi):=\left\{
 \begin{array}{lll}
   \cos(\mu_1x_2),& &x_2\in(0,h)\\
   & &\\
   \cos(\mu_1h)\cos[\mu_2(x_2-h)]-\frac{\mu_1}{\mu_2}\sin(\mu_1h)\sin[\mu_2(x_2-h)],& &x_2\in(h,+\infty)
 \end{array}
 \right.
 \end{eqnarray*}
\end{small}
\end{frame}

\begin{frame}
\frametitle{(ii)另一种选取方法：$G_{bp}(x,y)=G(x,y)$}
\begin{block}{有限孔穴点扩散函数：}
\begin{equation*}
\textcolor{red}{
  J_d(z,y):=\int_{\Gamma_0^d}\frac{\partial G(x_r,z)}{\partial x_2(x_r)}\overline{N(x_r,y)}ds(x_r)
  }
\end{equation*}
其中函数$G(x,z),N(x,y)$分别为Pekeris开波导Dirichlet和Neumann格林函数。
\end{block}
\begin{small}
