\begin{frame}
\frametitle{阻抗型开波导逆时偏移算法}
\begin{small}
\begin{algorithm}\label{alg_imp}
设$\Omega$为采样区域，令$u^s(x_r,x_s)$ 为在接收点$x_r$收到的由源点$x_s$所激发的散射数据，其中$x_r,x_s\in\Gamma_0^d;r=1,\ldots,N_r;s=1,\ldots,N_s$.
\begin{enumerate}
  \item 反传播： 对$s=1,\ldots,N_s$，计算反传播场
\begin{equation*}
  v_b(z,x_s)=\frac{|\Gamma_0^d|}{N_r}\sum\limits_{r=1}^{N_r}\frac{\partial \textcolor{red}{G_{\lambda}(x_r,z)}}{\partial x_2(x_r)}\overline{u^s(x_r,x_s)},\  \  \forall z\in\Omega.
\end{equation*}
若令$N_r\rightarrow0$，则上式可看做如下积分的数值近似，
\begin{equation*}
  \hat v_b(z,x_s)=\int_{\Gamma_0^d}\frac{\partial \textcolor{red}{G_{\lambda}(x_r,z)}}{\partial x_2(x_r)}\overline{u^s(x_r,x_s)}ds(x_r)
\end{equation*}
  \item 互相关： 对$z\in\Omega$，计算成像函数
\begin{equation*}
  I_d^{\lambda}(z)=\Im\left\{\frac{|\Gamma_0^d|}{N_s}\sum\limits_{s=1}^{N_s}\frac{\partial \textcolor{red}{G_{\lambda}(x_s,z)}}{\partial x_2(x_s)}v_b(z,x_s)
  \right\}.
\end{equation*}
将$v_b(z,x_s)$的表达式代入$I_d^{\lambda}(z)$，可得
\begin{equation*}\label{Id_imp}
  I_d^{\lambda}(z)=\Im\left\{\frac{|\Gamma_0^d|}{N_s}\frac{|\Gamma_0^d|}{N_r}\sum\limits_{s=1}^{N_s}\sum\limits_{r=1}^{N_r}\frac{\partial \textcolor{red}{G_{\lambda}(x_s,z)}}{\partial x_2(x_s)}\frac{\partial \textcolor{red}{G_{\lambda}(x_r,z)}}{\partial x_2(x_r)}\overline{u^s(x_r,x_s)}
  \right\}.
\end{equation*}
\end{enumerate}
\end{algorithm}
\end{small}
\end{frame}
\begin{frame}
\frametitle{障碍物成像比较：$D\subset L_1$，可穿透障碍物}
