\begin{frame}
\frametitle{逆时偏移算法：基本思想}
\begin{small}
\begin{algorithm}[半空间逆时偏移算法]
设$u^s(x_r,x_s)$为在接受点$x_r\in\Gamma_0^d$上所接受到的由点源$x_s\in\Gamma_0^d$ 所激发的散射场。其中$\Gamma_0^d:=\{(x_1,x_2)\in\R^2;x_1\in(-d,d),x_2=0\}$，$s=1,2,\ldots,N_s$，$r=1,2,\ldots,N_r$。然后算法具体实现分为如下两步：
\begin{enumerate}
  \item 反传播：对$s=1,2,\ldots,N_s$，计算反传播场
  \begin{equation*}
      v_b(z,x_s)=\frac{|\Gamma_0^d|}{N_r}\sum\limits_{r=1}^{N_r}\frac{\partial\Phi_k(x_r,z)}{\partial x_2(x_r)}\textcolor{red}{\overline{u^s(x_r,x_s)}},\  \  \forall z\in\Omega.
  \end{equation*}
  \item 互相关：对$z\in\Omega$，计算成像函数
  \begin{equation*}
    I_d(z)=\Im\left\{\frac{|\Gamma_0^d|}{N_s}\sum\limits_{s=1}^{N_s}\frac{\partial\Phi_k(x_s,z)}{\partial x_2(x_s)}v_b(z,x_s)
  \right\}.
\end{equation*}
\end{enumerate}
\end{algorithm} 
\begin{block}{}
将反传波场$v_b(z,x_s)$代入到成像函数$I_d(z)$，可得
\begin{equation*}
  \qquad I_d(z)=\Im\left\{\frac{|\Gamma_0^d||\Gamma_0^d|}{N_sN_r}
  \sum\limits_{s=1}^{N_s}\sum\limits_{r=1}^{N_r}
  \frac{\partial\Phi_k(x_s,z)}{\partial x_2(x_s)}\frac{\partial\Phi_k(x_r,z)}{\partial x_2(x_r)}
  \textcolor{red}{\overline{u^s(x_r,x_s)}}
  \right\},\  \ \forall z\in\Omega.\quad\quad
\end{equation*}
\end{block}
\end{small}
\end{frame}
\begin{frame}
\frametitle{逆时偏移算法：成像函数}
\begin{block}{成像函数}
\begin{small}
若令$N_s,N_r\rightarrow\infty$，则上式可看做对如下积分的近似。
\begin{equation*}
 \qquad  I_{\rm RTM}(z)=\Im\int_{\Gamma_0^d}\int_{\Gamma_0^d}\frac{\partial\Phi_{k}(x_s,z)}{\partial x_2(x_s)}\frac{\partial\Phi_{k}(x_r,z)}{\partial x_2(x_r)}
  \textcolor{red}{u^s(x_r,x_s)}ds(x_r)ds(x_s),\  \  \forall z\in\Omega.\quad\quad
\end{equation*}
\end{small}
\end{block}
