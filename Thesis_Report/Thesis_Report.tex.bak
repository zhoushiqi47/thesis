\section{半空间无相位数据障碍物成像问题}
\subsection{问题介绍}
\begin{frame}
\frametitle{半空间逆散射问题}
\begin{columns}
\begin{column}{.5\textwidth}

\begin{tiny}
\begin{eqnarray*}
\left\{
\begin{array}{lll}
\Delta u+k^2n(x)u=-\delta_{x_s}(x),&in&\R^2_+\\
& &\\
\frac{\partial u}{\partial x_2}=0,&on&\Gamma_0\\
& &\\
\sqrt{r}\left(\frac{\partial u}{\partial r}-\i ku\right)\rightarrow0,& as& r\rightarrow\infty,\ \ r=|x|
\end{array}
\right.
\end{eqnarray*}
\end{tiny}
\small{其中$n(x)-1$为支集在障碍物$D$上的有界函数。}
\end{column}

\begin{column}{.5\textwidth}
\begin{figure}
  \centering
  \includegraphics[width=6cm,height=3cm]{./figure/half_forward}
\end{figure}
\end{column}
\end{columns}
\begin{block}{散射问题：}
令$u^i(x,x_s)=N_k(x,x_s)$为入射场，其中$N_k(x,y)=\Phi_k(x,y)+\Phi_k(x,y')$为半空间Neumann格林函数，然后求解散射场$u^s(x,x_s)=u(x,x_s)-u^i(x,x_s)$。
\end{block}
\begin{block}{逆散射问题：}
\begin{enumerate}
  \item 通过在$\Gamma_0$上接受到的\textcolor{red}{散射数据}，构建算法来确定障碍物$D$的位置、大小和形状。
  \item 通过在$\Gamma_0$上接受到的\textcolor{red}{无相位数据}，构建算法来确定障碍物$D$的位置、大小和形状。
\end{enumerate}
\end{block}
\end{frame}
\subsection{求解算法}
\begin{frame}
\frametitle{求解算法}
\begin{small}
\begin{algorithm}[半空间无相位逆时偏移算法]
设$\textcolor{red}{|u(x_r,x_s)|=|u^s(x_r,x_s)+u^i(x_r,x_s)|}$ 为我们在接收点$x_r\in\Gamma_0^d$出采集的无相位总场数据，其源点为$x_s\in\Gamma_0^d$，并令$\Omega$为采样区域。
\begin{enumerate}
  \item 反传播： 对于 $s=1,\ldots,N_s$，计算反传播场
\begin{equation*}
  v_b(z,x_s)=\frac{|\Gamma_0^d|}{N_r}\sum\limits_{r=1}^{N_r}\frac{\partial\Phi_k(x_r,z)}{\partial x_2(x_r)}\textcolor{red}{\Delta(x_r,x_s)},\  \  \forall z\in\Omega.
\end{equation*}
其中
\begin{equation*}
\textcolor{red}{
     \Delta(x_r,x_s)=\frac{|u(x_r,x_s)|^2-|u^i(x_r,x_s)|^2}{u^i(x_r,x_s)}.
     }
\end{equation*}
  \item 互相关: 对于 $z\in\Omega$, 计算成像函数
\begin{equation*}
  I_d(z)=\Im\left\{\frac{|\Gamma_0^d|}{N_s}\sum\limits_{s=1}^{N_s}\frac{\partial\Phi_k(x_s,z)}{\partial x_2(x_s)}v_b(z,x_s)
  \right\}.
\end{equation*}
\end{enumerate}

将反传波场$v_b(z,x_s)$代入到成像函数$I_d(z)$，可得
\begin{equation*}
  \qquad I_d(z)=\Im\left\{\frac{|\Gamma_0^d||\Gamma_0^d|}{N_sN_r}
  \sum\limits_{s=1}^{N_s}\sum\limits_{r=1}^{N_r}
  \frac{\partial\Phi_k(x_s,z)}{\partial x_2(x_s)}\frac{\partial\Phi_k(x_r,z)}{\partial x_2(x_r)}
  \textcolor{red}{\Delta(x_r,x_s)}
  \right\},\  \ \forall z\in\Omega.\quad\quad
\end{equation*}
\end{algorithm}
\end{small}
\end{frame}
\begin{frame}
\frametitle{算法比较}
\begin{small}
若令$N_s,N_r\rightarrow\infty$，则上式可看做对如下积分的近似。
\begin{equation*}
 \qquad  I^{\rm Phaseless}_{\rm RTM}(z)=\Im\int_{\Gamma_0^d}\int_{\Gamma_0^d}\frac{\partial\Phi_k(x_s,z)}{\partial x_2(x_s)}\frac{\partial\Phi_k(x_r,z)}{\partial x_2(x_r)}
  \textcolor{purple}{\Delta(x_r,x_s)}ds(x_r)ds(x_s),\  \  \forall z\in\Omega.\quad\quad
\end{equation*}
观察到
\begin{equation*}
  \textcolor{purple}{\Delta(x_r,x_s)}=\textcolor{red}{\overline{u^s(x_r,x_s)}}+
  \textcolor{blue}{\frac{|u^s(x_r,x_s)|^2}{u^i(x_r,x_s)}}+\textcolor{blue}{\frac{u^s(x_r,x_s)
  \overline{u^i(x_r,x_s)}}{u^i(x_r,x_s)}}.
\end{equation*}
据此可得
\begin{eqnarray*}
 I^{\rm Phaseless}_{\rm RTM}(z)&=& \Im\int_{\Gamma_{0}^d}\int_{\Gamma_0^d}\frac{\partial\Phi_k(x_s,z)}{\partial x_2(x_s)}\frac{\partial\Phi_k(x_r,z)}{\partial x_2(x_r)}
  \overline{u^s(x_r,x_s)}  ds(x_r)ds(x_s)\\
  & &+ \Im\int_{\Gamma_{0}^d}\int_{\Gamma_0^d}\frac{\partial\Phi_k(x_s,z)}{\partial x_2(x_s)}\frac{\partial\Phi_k(x_r,z)}{\partial x_2(x_r)}\frac{|u^s(x_r,x_s)|^2}{u^i(x_r,x_s)}ds(x_r)ds(x_s)\\
  & & +\Im\int_{\Gamma_{0}^d}\int_{\Gamma_0^d}\frac{\partial\Phi_k(x_s,z)}{\partial x_2(x_s)}\frac{\partial\Phi_k(x_r,z)}{\partial x_2(x_r)}
  \frac{u^s(x_r,x_s)
  \overline{u^i(x_r,x_s)}}{u^i(x_r,x_s)}ds(x_r)ds(x_s)\\
  & &\\
  &:=& \textcolor{red}{I_{\rm RTM}(z)}+\textcolor{blue}{{\rm I}_2(z)}
  +\textcolor{blue}{{\rm I}_3(z)}
\end{eqnarray*}
\end{small}
\end{frame}

