\begin{frame}
\frametitle{收敛性问题}
\begin{block}{}
\begin{equation*}
  J_d^1(z,y)=\sum\limits_{p=1}^M\sum\limits_{q=1}^{\hat M}f_{pq}(z_2,y_2)\textcolor{red}{I^{pq}_d(z_1,y_1)}
\end{equation*}
其中$f_{pq}(z_2,y_2),p=1,2\ldots,M;q=1,2,\ldots,\hat M$ 是关于$z_2$和$y_2$的函数，以及
\begin{equation*}
\textcolor{red}{
    I^{pq}_d(z_1,y_1) =\int_{-d}^{d}e^{\i|x_1-z_1|\xi_p-\i|x_1-y_1|\hat\xi_q}dx_1
}
\end{equation*}
\end{block}
\begin{remark}\label{Idm1m2}
直接计算可得$I_d^{pq}(z_1,y_1)$的具体表达式：当$z_1,y_1\in(-d,d)$时，
\begin{equation*}
  I_d^{pq}(z_1,y_1)=\textcolor{blue}{\psi^{pq}_1(d,z_1,y_1)}+\psi^{pq}_2(z_1,y_1)
\end{equation*}
其中
\begin{eqnarray*}
\left\{
\begin{array}{lll}
  \textcolor{blue}{\psi^{pq}_1(d,z_1,y_1)}&=&\frac{\textcolor{red}{e^{\i d(\xi_{p}-\hat\xi_{q})}}}{\i(\xi_{p}-\hat\xi_{q})}\left(e^{\i z_1\xi_{p}-\i y_1\hat\xi_{q}}+e^{\i y_1\hat\xi_{q}-\i z_1\xi_{p}}\right)\\
  & &\\
  \psi^{pq}_2(z_1,y_1)&=&\frac{e^{\i|z_1-y_1|\xi_{p}}-e^{-\i|z_1-y_1|\hat\xi_{q}}}{\i(\xi_{p}+\hat\xi_{q})}
-\frac{e^{\i|z_1-y_1|\xi_{p}}+e^{-\i|z_1-y_1|\hat\xi_{q}}}{\i(\xi_{p}-\hat\xi_{q})}
\end{array}
\right.
\end{eqnarray*}
\end{remark}
\end{frame}


\begin{frame}
\frametitle{解决方案一：取$G_{bp}(x,y)=G_h(x,y)$}
\begin{small}
\begin{block}{全空间两层基本解$G_h(x,y)$}
如果仅考虑Pekeris半空间两层开波导障碍物成像问题，可以考虑采用全空间两层基本解$G_h(x,y)$作为一种备选的反传播函数。 $G_h(x,y)$ 满足如下方程：
\begin{eqnarray*}
\left\{
\begin{array}{lll}
  \Delta_xG_h(x,y)+\hat k^2(x)G_h(x,y)=-\delta_y(x),& in& \R^2\\
  & &\\
  \left[G_h(\cdot,y)\right]_{\Gamma_h}=\left[\frac{\partial G_h(\cdot,y)}{\partial\nu}\right]_{\Gamma_h}=0.
    \end{array}
\right.
\end{eqnarray*}
其中波数$\hat k(x)$满足
\begin{eqnarray*}
\hat k(x)=\left\{
\begin{array}{lll}
  k_1&,&x_2<h\\
  k_2&,&x_2>h
\end{array}
\right.
\end{eqnarray*}
\end{block}
\end{small}

\begin{small}
\begin{block}{特点}
\begin{itemize}
  \item 好处：不会产生波导模式，且表达式相对简单，数值和理论分析较为容易
  \item 坏处：不符合逆时偏移的思想，不能推广到半空间任意层模型，仅对两层内的障碍物有效
\end{itemize}
\end{block}
\end{small}
\end{frame}

\begin{frame}
\frametitle{函数$G_h(x,z)$的积分表达式}
\begin{small}
\begin{block}{}
直接计算可知，函数$G_h(x,y)$表达式如下
\begin{eqnarray*}
G_h(x,y)=\left\{
 \begin{array}{lll}
 \Phi(k_1,x,y)-\Phi(k_1,x,y^*)+P_{11}(x,y),& &y_2<h,x_2<h\\
 P_{12}(x,y),& &y_2<h,x_2>h\\
 P_{21}(x,y),& &y_2>h,x_2<h\\
 \Phi(k_2,x,y)-\Phi(k_2,x,y^*)+P_{22}(x,y),& &y_2>h,y_2>h
 \end{array}
 \right.
\end{eqnarray*}
其中$y^*=(y_1,2h-y_2)$，$\mu_j=\sqrt{k_j^2-\xi^2}, j=1,2$，以及函数$P_{ij}(x,y)$的表达式为
\begin{eqnarray*}
 \left\{
 \begin{array}{lll}
 P_{11}(x,y)&=&\frac{\i}{2\pi}\int_{SIP}\frac{1}{\textcolor{red}{\mu_1+\mu_2}}e^{\i\mu_1(2h-x_2-y_2)+\i\xi(x_1-y_1)}d\xi\\
 & &\\
 P_{12}(x,y)&=&\frac{\i}{2\pi}\int_{SIP}\frac{1}{\textcolor{red}{\mu_1+\mu_2}}e^{\i\mu_1(h-y_2)+\i\mu_2(x_2-h)+\i\xi(x_1-y_1)}d\xi\\
 & &\\
 P_{21}(x,y)&=&\frac{\i}{2\pi}\int_{SIP}\frac{1}{\textcolor{red}{\mu_1+\mu_2}}e^{\i\mu_2(y_2-h)+\i\mu_1(h-x_2)+\i\xi(x_1-y_1)}d\xi\\
 & &\\
 P_{22}(z,y)&=&\frac{\i}{2\pi}\int_{SIP}\frac{1}{\textcolor{red}{\mu_1+\mu_2}}e^{\i\mu_2(x_2+y_2-2h)+\i\xi(x_1-y_1)}d\xi
 \end{array}
 \right.
\end{eqnarray*}
\end{block}
\end{small}
\end{frame}

\begin{frame}
\frametitle{解决方案二：取$G_{bp}(x,y)=G_{\lambda}(x,y)$}
\begin{small}
\begin{block}{半空间两层介质Impedance格林函数$G_{\lambda}(x,y)$}
设常数$\lambda$为阻抗系数，函数$G_{\lambda}(x,y)$满足如下方程：
\begin{eqnarray*}\label{G_Impedance}
\left\{
\begin{array}{lll}
  \Delta_xG_{\lambda}(x,y)+k^2(x)G_{\lambda}(x,y)=-\delta_y(x),&in&\quad \R^2_+\\
  & &\\
  \left[G_{\lambda}(\cdot,y)\right]_{\Gamma_h}=0,\ \ \left[\frac{\partial G_{\lambda}(\cdot,y)}{\partial\nu}\right]_{\Gamma_h}=0 & &\\
  & &\\
  \frac{\partial G_{\lambda}(x,y)}{\partial x_2}+\i\lambda G_{\lambda}(x,y)=0, &on&\Gamma_0\\
\end{array}
\right.
\end{eqnarray*}
其中
\begin{eqnarray*}\label{owg_wn}
k(x)=\left\{
\begin{array}{lll}
  k_1&,&x_2\in(0,h)\\
  k_2&,&x_2\in(h,+\infty)
\end{array}
\right.
\end{eqnarray*}

\end{block}
\end{small}
\begin{small}
\begin{block}{特点}
\begin{itemize}
  \item 好处：多了一个可调参数$\lambda$，当$\lambda$取值适当时不会产生波导模式；可以推广到半空间任意层开波导障碍物成像问题；符合逆时偏移算法的基本思想
  \item  坏处：表达式较为复杂，数值和理论分析较为困难
\end{itemize}
\end{block}
\end{small}
\end{frame}

\begin{frame}
\frametitle{函数$G_{\lambda}(x,y)$的积分表达式}
\begin{small}
\begin{block}{}
设$\lambda>0$，则半空间两层介质Impedance格林函数$G_{\lambda}(x,y)$表达式为
\begin{eqnarray*}\label{IFT_impedance}
  G_{\lambda}(x,y)&=&\frac{1}{2\pi}\int_{SIP}\hat G^y_{\lambda}(\xi,x_2)e^{\i|x_1-y_1|\xi}d\xi
\end{eqnarray*}
其中$\hat{G}^y_{\lambda}(\xi,x_2)$表示函数$G_{\lambda}^y(x_1,x_2):=G_{\lambda}(x,y)$ 关于$x$ 的
第一个分量$x_1$的Fourier变换：
\begin{equation*}
 \hat{G}^y_{\lambda}(\xi,x_2)=\int^{\infty}_{-\infty}G^y_{\lambda}(x_1,x_2)e^{-\i(x_1-y_1)\xi}dx_1
\end{equation*}
\end{block}
\end{small}

\begin{small}
\begin{block}{}
\begin{eqnarray*}
\left\{
\begin{array}{lll}
 \frac{\partial^2\hat{G}^y_{\lambda}(\xi,x_2)}{\partial x^2_2}+[k^2(x)-\xi^2]\hat{G}^y_{\lambda}(\xi,x_2)=-\delta_{y_2}(x_2),&in&\quad \R_+\\
 & &\\
 \left[\hat{G}^y_{\lambda}(\xi,x_2)\right]_{x_2=h}=\left[\frac{\partial\hat{G}^y_{\lambda}(\xi,x_2)}{\partial x_2}\right]_{x_2=h}=0,& &\\
 & &\\
 \frac{\partial \hat{G}^y_{\lambda}(\xi,x_2)}{\partial x_2}+\i\lambda\hat{G}^y_{\lambda}(\xi,x_2)=0,&on &x_2=0
\end{array}
\right.
\end{eqnarray*}
\end{block}
\end{small}
\end{frame}

\begin{frame}
\frametitle{函数$G_{\lambda}(x,y)$的Fourier变换$\hat{G}^y_{\lambda}(\xi,x_2)$}
\begin{small}
记关于$\xi$的函数$N_{\lambda}(\xi),M_{\lambda}(\xi)$为：
\begin{eqnarray*}
\textcolor{red}{
  N_{\lambda}(\xi)=\frac{\mu_1+\mu_2}{\mu_1-\mu_2}\frac{\mu_1+\lambda}{\mu_1-\lambda}-e^{2\i\mu_1h}},\ \
  M_{\lambda}(\xi)=\frac{\lambda+\mu_1}{\lambda-\mu_1}+\frac{\mu_1+\mu_2}{\mu_1-\mu_2}e^{2\i\mu_1h}
\end{eqnarray*}
(1) 当源点$y\in L_1$时，若$x_2\in(0,h)$，
\begin{eqnarray*}
  \hat{G}_{\lambda}^y(\xi,x_2)=\left\{
\begin{array}{lll}
  \frac{\i}{2\mu_1}e^{\i\mu_1|x_2-y_2|}&+&\frac{\i}{2\mu_1}\frac{\mu_1-\lambda}{\mu_1+\lambda}e^{\i\mu_1(x_2+y_2)}\\
  & &\\
  +\frac{\i}{2\mu_1}\frac{e^{2\i\mu_1h}}{\textcolor{red}{N_{\lambda}(\xi)}}\frac{\mu_1-\lambda}{\mu_1+\lambda}e^{\i\mu_1(x_2+y_2)}
  &+&\frac{\i}{2\mu_1}\frac{e^{2\i\mu_1h}}{\textcolor{red}{N_{\lambda}(\xi)}}e^{\i\mu_1(x_2-y_2)}\\
  & &\\
  +\frac{\i}{2\mu_1}\frac{e^{2\i\mu_1h}}{\textcolor{red}{N_{\lambda}(\xi)}}\frac{\mu_1+\lambda}{\mu_1-\lambda}e^{\i\mu_1(-x_2-y_2)}
  &+&\frac{\i}{2\mu_1}\frac{e^{2\i\mu_1h}}{\textcolor{red}{N_{\lambda}(\xi)}}e^{\i\mu_1(-x_2+y_2)}
\end{array}
\right.
\end{eqnarray*}
(2) 当源点$y\in L_1$时，若$x_2\in(h,+\infty)$，
\begin{eqnarray*}
 \begin{array}{lll}
    \hat{G}_{\lambda}^y(\xi,x_2)&=&\frac{\i}{\mu_1-\mu_2}\frac{1}{\textcolor{red}{N_{\lambda}(\xi)}}\left[\frac{\mu_1+\lambda}{\mu_1-\lambda}e^{\i\mu_1(h-y_2)+\i\mu_2(x_2-h)}+
  e^{\i\mu_1(h+y_2)+\i\mu_2(x_2-h)}\right]
 \end{array}
\end{eqnarray*}
(3) 当源点$y\in L_2$时，若$x_2\in(0,h)$，
\begin{eqnarray*}
 \begin{array}{lll}
    \hat{G}_{\lambda}^y(\xi,x_2)&=&\frac{\i}{\mu_1-\mu_2}\frac{1}{\textcolor{red}{N_{\lambda}(\xi)}}\left[e^{\i\mu_2(y_2-h)+\i\mu_1(x_2+h)}+
 \frac{\mu_1+\lambda}{\mu_1-\lambda}e^{\i\mu_2(y_2-h)+\i\mu_1(h-x_2)}\right]
 \end{array}
\end{eqnarray*}
(4) 当源点$y\in L_2$时，若$x_2\in(h,+\infty)$，
\begin{eqnarray*}
 \begin{array}{lll}
    \hat{G}_{\lambda}^y(\xi,x_2)&=&\frac{\i}{2\mu_2}e^{\i\mu_2|x_2-y_2|}
  +\frac{\i}{2\mu_2}\frac{M_{\lambda}(\xi)}{\textcolor{red}{N_{\lambda}(\xi)}}e^{\i\mu_2(x_2+y_2-2h)}
 \end{array}
\end{eqnarray*}
\end{small}
\end{frame}


\begin{frame}
\frametitle{回顾：开波导Dirichlet格林函数$G(x,y)$的积分表达式}
\begin{small}
令$\xi=\xi_1+\i\xi_2,\xi_1,\xi_2\in\R$，以及$\mu_j=\sqrt{k_j^2-\xi^2},j=1,2$，并记关于$\xi$的函数$N_h(\xi)$和$M_h(\xi)$如下所示
\begin{eqnarray*}\label{NhMh}
 \textcolor{red}{N_h(\xi)=\frac{\mu_1+\mu_2}{\mu_1-\mu_2}+e^{2\i\mu_1h}},\ \
 M_h(\xi)=1+\frac{\mu_1+\mu_2}{\mu_1-\mu_2}e^{2\i\mu_1h},
\end{eqnarray*}
则格林函数$G(x,y)$有如下表达式
\begin{eqnarray*}
G(x,y)=\left\{
\begin{array}{lll}
  \Phi_{k_1}(x,y)-\Phi_{k_1}(x,y')+S_1(x,y)&, &x\in L_1,y\in L_1\\
 S_2(x,y)&, &x\in L_2,y\in L_1\\
 K_1(x,y)&, &x\in L_1,y\in L_2\\
 \Phi_{k_2}(x,y)+K_2(x,y)&,&x\in L_2,y\in L_2
\end{array}
\right.
\end{eqnarray*}
其中$S_j(x,y),K_j(x,y),j=1,2$ 的表达式为
\begin{eqnarray*}
\left\{
\begin{array}{lll}
  S_1(x,y)&=&\frac{1}{2\pi}\int_{SIP}\frac{-\i}{2\mu_1}\frac{e^{2\i\mu_1h}\left[4\sin{(\mu_1x_2)\sin{(\mu_1y_2)}}\right]}{\textcolor{red}{N_h(\xi)}}
  e^{\i(x_1-y_1)\xi}d\xi\\
  & &\\
  S_2(x,y)&=&\frac{1}{2\pi}\int_{SIP}\frac{2e^{\i\mu_1h}\sin{(\mu_1y_2)}}{(\mu_1-\mu_2)\textcolor{red}{N_h(\xi)}}e^{\i\mu_2(x_2-h)+\i(x_1-y_1)\xi}d\xi\\
  & &\\
  K_1(x,y)&=&\frac{1}{2\pi}\int_{SIP}\frac{2e^{\i\mu_1h}\sin(\mu_1x_2)}{(\mu_1-\mu_2)\textcolor{red}{N_h(\xi)}}e^{\i\mu_2(y_2-h)+\i(x_1-y_1)\xi}d\xi\\
  & &\\
  K_2(x,y)&=&\frac{1}{2\pi}\int_{SIP}\frac{-\i}{2\mu_2}\frac{M_h(\xi)}{\textcolor{red}{N_h(\xi)}}e^{\i\mu_2(x_2+y_2-2h)+\i(x_1-y_1)\xi}d\xi
\end{array}
\right.
\end{eqnarray*}
\end{small}

\end{frame}

\begin{frame}
%{|开波导Dirichlet格林函数$G(x,y)$|阻抗格林函数$G_{\lambda}(x,y)$|全空间两层基本解$G_h(x,y)$}
\frametitle{比较}
\begin{block}{反传播函数}
\begin{eqnarray*}
\left\{
\begin{array}{lll}
\mbox{开波导Dirichlet格林函数}G(x,y)&:&N_h(\xi)=\frac{\mu_1+\mu_2}{\mu_1-\mu_2}+e^{2\i\mu_1h}\\
& &\\
\mbox{半空间两层阻抗格林函数}G_{\lambda}(x,y)&:&
N_{\lambda}(\xi)=\frac{\mu_1+\mu_2}{\mu_1-\mu_2}\textcolor{blue}{\frac{\mu_1+\lambda}{\mu_1-\lambda}}-e^{2\i\mu_1h}\\
& &\\
\mbox{全空间两层基本解}G_h(x,y)&:&\mu_1+\mu_2
\end{array}
\right.
\end{eqnarray*}
\end{block}

\begin{block}{有限孔穴点扩散函数}
\begin{enumerate}
  \item $  J_d(z,y):=\int_{\Gamma_0^d}\frac{\partial\textcolor{red}{ G(x_r,z)}}{\partial x_2(x_r)}\overline{N(x_r,y)}ds(x_r)$\hspace{2cm} $\surd$
  \item $J_d(z,y):=\int_{\Gamma_0^d}\frac{\partial\textcolor{red}{ G_{\lambda}(x_r,z)}}{\partial x_2(x_r)}\overline{N(x_r,y)}ds(x_r)$\hspace{1.9cm} ?

  \item $J_d(z,y):=\int_{\Gamma_0^d}\frac{\partial\textcolor{red}{ G_h(x_r,z)}}{\partial x_2(x_r)}\overline{N(x_r,y)}ds(x_r)$ \hspace{1.8cm} ?
\end{enumerate}

其中函数$N(x,y)$为Pekeris开波导Neumann格林函数。

\end{block}
\end{frame}


